#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/smsflash/flashmonitor__node/node_modules/.pnpm/google-gax@3.6.1_encoding@0.1.13/node_modules/google-gax/build/tools/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/google-gax@3.6.1_encoding@0.1.13/node_modules/google-gax/build/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/google-gax@3.6.1_encoding@0.1.13/node_modules/google-gax/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/google-gax@3.6.1_encoding@0.1.13/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/smsflash/flashmonitor__node/node_modules/.pnpm/google-gax@3.6.1_encoding@0.1.13/node_modules/google-gax/build/tools/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/google-gax@3.6.1_encoding@0.1.13/node_modules/google-gax/build/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/google-gax@3.6.1_encoding@0.1.13/node_modules/google-gax/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/google-gax@3.6.1_encoding@0.1.13/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/tools/minify.js" "$@"
else
  exec node  "$basedir/../../build/tools/minify.js" "$@"
fi
