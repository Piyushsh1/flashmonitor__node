#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/smsflash/flashmonitor__node/node_modules/.pnpm/babel-watch@7.8.1_@babel+core@7.23.9/node_modules/babel-watch/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/babel-watch@7.8.1_@babel+core@7.23.9/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/smsflash/flashmonitor__node/node_modules/.pnpm/babel-watch@7.8.1_@babel+core@7.23.9/node_modules/babel-watch/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/babel-watch@7.8.1_@babel+core@7.23.9/node_modules:/home/smsflash/flashmonitor__node/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../babel-watch/babel-watch.js" "$@"
else
  exec node  "$basedir/../babel-watch/babel-watch.js" "$@"
fi
